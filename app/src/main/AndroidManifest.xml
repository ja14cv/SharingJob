<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jcaal.sharingjob_v01" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".principal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".nuevo_empleo"
            android:label="@string/title_activity_nuevo_empleo"
            android:parentActivityName=".principal" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jcaal.sharingjob_v01.principal" />
        </activity>
        <activity
            android:name=".mi_cuenta"
            android:label="@string/title_activity_mi_cuenta"
            android:parentActivityName=".principal" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jcaal.sharingjob_v01.principal" />
        </activity>
    </application>

</manifest>
